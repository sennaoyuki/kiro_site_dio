{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/editorial-policy",
      "destination": "/editorial-policy.html"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/company",
      "destination": "/company.html"
    },
    {
      "source": "/privacy-policy",
      "destination": "/privacy-policy.html"
    }
  ]
}